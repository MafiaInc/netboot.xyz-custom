#!ipxe

###
### netboot.xyz-wtware menu
###

:custom
clear custom_choice
menu WTware boot
item --gap Boot WTware submenu
item option_one ${space} Loading a kernel and initrd
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:option_one
set wtware-url http://192.168.1.224:8080/wtware/
boot   ${wtware-url}/5.8.88/packages/kernel BOOT_IMAGE=${wtware-url}/5.8.88/packages/kernel quiet

:custom_exit
chain utils.ipxe
exit

